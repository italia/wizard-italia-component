{
  "name": "wizard-italia",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "demo": "vue-cli-service serve",
    "build-lib": "rm -rf ./dist && vue-cli-service build --target lib --name wizard-italia src/entry.js",
    "build-wc": "rm -rf ./dist && vue-cli-service build --target lib --name wizard-italia src/wc.js",
    "build-all": "yarn build-wc && mv dist wc && yarn build-lib && mv wc ./dist",
    "lint": "vue-cli-service lint",
    "prepare-wc-demo": "cp -R ./dist ./demo"
  },
  "dependencies": {
    "marked": "^3.0.3"
  },
  "peerDependencies": {
    "@oruga-ui/oruga": "^0.4.4",
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@oruga-ui/oruga": "^0.4.4",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "@fontsource/titillium-web": "^4.3.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.13",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "bootstrap-italia": "^1.4.3",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.1.0",
    "js-yaml": "^4.1.0",
    "json-loader": "^0.5.7",
    "vue-cli-plugin-webpack-bundle-analyzer": "^4.0.0",
    "vue-custom-element": "^3.3.0",
    "vue-template-compiler": "^2.6.11"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/wizard-italia.common.js",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
